#: ../../Basic User's Guide/index_usersguide.rst:27
msgid "Hash Chains"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:29
msgid "**ID**                    - Hash Chain ID, corresponds with the labeling on the control board"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:30
msgid "**REAL HASH RATE**        - Actual hash rate of the hash chain"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:31
msgid "**NOMINAL HASH RATE**     - Theoretical hash rate of the hash chain, based on the frequency of the chips"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:32
msgid "**VOLTAGE**               - Voltage used on the hash chain"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:33
msgid "**FREQUENCY**             - Frequency of the chips (average)"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:34
msgid "**BOARD TEMP**            - Temperature reported by the sensors on the hash board"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:35
msgid "**CHIP TEMP**             - Temperature reported by the sensors on the chip"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:36
msgid "**ASIC#**                 - Number of functioning ASIC chips"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:37
msgid "**CORE#**                 - Summary of active cores on all functioning chips"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:38
msgid "**HARDWARE ERRORS**       - Number of hardware faults - invalid work due to a hardware miscalculation"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:39
msgid "**HW ERROR HASH RATE**    - Hash rate \"loss\" due to HW errors; adding HW Error Hash Rate together with the Real Hash Rate should equal the Nominal Hash Rate"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:42
msgid "Pools"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:44
msgid "**ID**                    - Pool order, as specified by the user"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:45
msgid "**URL**                   - Mining pool URL address"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:46
msgid "**USER**                  - Username and worker name, as specified by the user"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:47
msgid "**STATUS**                - Status of the pool - \"Alive\" when the pool is reachable by the miner, \"Dead\" when the pool is not reachable on that URL"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:48
msgid "**ACTIVE**                - Active status: \"Yes\" - pool is being used to serve mining jobs; \"No\" - pool is not used"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:49
#: ../../Basic User's Guide/index_usersguide.rst:63
msgid "**ACCEPTED**              - Number of submited shares that were accepted by the pool"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:50
#: ../../Basic User's Guide/index_usersguide.rst:65
msgid "**REJECTED**              - Number of submited shares that were rejected by the pool"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:51
msgid "**STALE**                 - Number of submited shares for a job that is no longer valid"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:52
msgid "**LAST DIFFICULTY**       - Last share difficulty"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:53
msgid "**GENERATED WORK**        - Amount of generated work for the chips to solve"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:54
msgid "**ASICBOOST**             - AsicBoost status - \"Yes\" for enabled, \"No\" for disabled"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:57
msgid "Summary"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:59
msgid "**HASH RATE 1M**          - Average hash rate for the last 1 minute"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:60
msgid "**HASH RATE 15M**         - Average hash rate for the last 15 minutes"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:61
msgid "**HASH RATE 24H**         - Average hash rate for the last 24 hours"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:62
msgid "**FOUND BLOCKS**          - Number of blocks found"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:64
msgid "**DIFFICULTY ACCEPTED**   - Difficulty of the last accepted share"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:66
msgid "**DIFFICULTY REJECTED**   - Difficulty of the last rejected share"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:67
msgid "**REJECTION RATIO**       - Ratio of rejected shares and total number of shares (including accepted)"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:68
msgid "**ELAPSED TIME**          - Elapsed time since BOSminer started"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:69
msgid "**HARDWARE ERRORS**       - Number of hardware faults - rejected work due to a hardware miscalculation"
msgstr ""

#: ../../Basic User's Guide/index_usersguide.rst:70
msgid "**SHARES/1M**             - Average amount of shares accepted per minute"
msgstr ""

